  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jwjacobson/jazztunes/issues"
  },
  "homepage": "https://github.com/jwjacobson/jazztunes#readme",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.8"
  },
  "dependencies": {
    "datatables.net": "^2.3.7",
    "datatables.net-dt": "^2.3.7",
    "htmx.org": "^2.0.8",
    "jquery": "^4.0.0"
