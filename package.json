{
  "name": "jazztunes",
  "version": "1.0.0",
  "description": "[![Documentation Status](https://readthedocs.org/projects/jazztunes/badge/?version=latest)](https://jazztunes.readthedocs.io/en/latest/) # jazztunes - a jazz repertoire management app",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build-css": "postcss ./static/src/input.css -o ./static/css/main.css",
    "watch-css": "postcss ./static/src/input.css -o ./static/css/main.css --watch",
    "copy-vendor": "bash copy_vendor.sh",
    "build": "npm run copy-vendor && npm run build-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwjacobson/jazztunes.git"
  },
  "keywords": [],
  "author": "",
